@model ProjeYonetimSistemi.UI.MVC.Entity.TeamEntity

<div class="content-wrapper" style="min-height: 1604.44px;">
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Takım Detayı</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="@Url.Action("Index", "Team")">Takımlar</a></li>
                        <li class="breadcrumb-item active">Takım Detayı</li>
                    </ol>
                </div>
            </div>
        </div>
    </section>

    <section class="content">
        <div class="card">
            <div class="card-header">
                <h3 class="card-title">Takım Detayları</h3>
            </div>
            <div class="card-body">
                <div class="form-group">
                    <label>Takım İsmi</label>
                    <p>@Model.TeamName</p>
                </div>
                <div class="form-group">
                    <label>Üyeler</label>
             
                </div>
                <a href="@Url.Action("Index", "Team")" class="btn btn-warning">Geri Dön</a>
            </div>
        </div>
    </section>
</div>
