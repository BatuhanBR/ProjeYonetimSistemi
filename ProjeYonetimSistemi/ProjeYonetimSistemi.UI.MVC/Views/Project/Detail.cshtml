@model dynamic

<div class="content-wrapper" style="min-height: 1604.44px;">
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Proje Detayı</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="@Url.Action("Index", "Project")">Projeler</a></li>
                        <li class="breadcrumb-item active">Proje Detayı</li>
                    </ol>
                </div>
            </div>
        </div>
    </section>

    <section class="content">
        <div class="card">
            <div class="card-header">
                <h3 class="card-title">Proje Detayları</h3>
            </div>
            <div class="card-body">
                <div class="form-group">
                    <label>Proje İsmi</label>
                    <p>@Model.ProjectName</p>
                </div>
                <div class="form-group">
                    <label>Açıklama</label>
                    <p>@Model.Description</p>
                </div>
                <a href="@Url.Action("Index", "Project")" class="btn btn-warning">Geri Dön</a>
            </div>
        </div>
    </section>
</div>
